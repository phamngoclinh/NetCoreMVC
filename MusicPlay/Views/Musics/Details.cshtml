@model MusicPlay.Models.Music

@{
    ViewData["Title"] = "Details";
}

<h2>@Html.DisplayFor(model => model.Title)</h2>

<div class="media">
    <div class="media-left media-middle">
        <a asp-action="Edit" asp-route-id="@Model.ID">
            <img class="media-object" src="data:image/jpeg;base64," alt="...">
        </a>
    </div>
    <div class="media-body">
        <h4 class="media-heading">@Html.DisplayFor(model => model.Title)</h4>
        <p>Upload: @Html.DisplayFor(model => model.ReleaseDate)</p>
        <p>Genren: @Html.DisplayFor(model => model.Genre)</p>
        <p>Rating: @Html.DisplayFor(model => model.Rating)</p>
    </div>
</div>

